        "svgMarkup": "\n  <!-- Traffic light pole -->\n  <rect xmlns=\"http://www.w3.org/2000/svg\" x=\"90\" y=\"280\" width=\"20\" height=\"120\" fill=\"#666\" stroke=\"#444\" stroke-width=\"2\"></rect>\n  \n  <!-- Traffic light housing -->\n  <rect xmlns=\"http://www.w3.org/2000/svg\" x=\"50\" y=\"50\" width=\"100\" height=\"240\" rx=\"15\" ry=\"15\" fill=\"#333\" stroke=\"#222\" stroke-width=\"3\"></rect>\n  \n  <!-- Housing details -->\n  <rect xmlns=\"http://www.w3.org/2000/svg\" x=\"55\" y=\"55\" width=\"90\" height=\"230\" rx=\"10\" ry=\"10\" fill=\"#444\"></rect>\n  \n  <!-- Light housings -->\n  <circle xmlns=\"http://www.w3.org/2000/svg\" cx=\"100\" cy=\"100\" r=\"30\" fill=\"#222\" stroke=\"#111\" stroke-width=\"2\"></circle>\n  <circle xmlns=\"http://www.w3.org/2000/svg\" cx=\"100\" cy=\"170\" r=\"30\" fill=\"#222\" stroke=\"#111\" stroke-width=\"2\"></circle>\n  <circle xmlns=\"http://www.w3.org/2000/svg\" cx=\"100\" cy=\"240\" r=\"30\" fill=\"#222\" stroke=\"#111\" stroke-width=\"2\"></circle>\n  \n  <!-- Red Light -->\n  <circle xmlns=\"http://www.w3.org/2000/svg\" cx=\"100\" cy=\"100\" r=\"25\" fill=\"#800000\" stroke=\"#600000\" stroke-width=\"2\">\n    <!-- Red light animation: on for 3s, off for 6s -->\n    <animate attributeName=\"fill\" values=\"#FF0000;#FF0000;#800000;#800000;#800000;#800000\" dur=\"9s\" repeatCount=\"indefinite\"></animate>\n    <animate attributeName=\"opacity\" values=\"1;1;0.3;0.3;0.3;0.3\" dur=\"9s\" repeatCount=\"indefinite\"></animate>\n  </circle>\n  \n  <!-- Yellow Light -->\n  <circle xmlns=\"http://www.w3.org/2000/svg\" cx=\"100\" cy=\"170\" r=\"25\" fill=\"#806000\" stroke=\"#604000\" stroke-width=\"2\">\n    <!-- Yellow light animation: off for 3s, on for 3s, off for 3s -->\n    <animate attributeName=\"fill\" values=\"#806000;#806000;#FFFF00;#FFFF00;#806000;#806000\" dur=\"9s\" repeatCount=\"indefinite\"></animate>\n    <animate attributeName=\"opacity\" values=\"0.3;0.3;1;1;0.3;0.3\" dur=\"9s\" repeatCount=\"indefinite\"></animate>\n  </circle>\n  \n  <!-- Green Light -->\n  <circle xmlns=\"http://www.w3.org/2000/svg\" cx=\"100\" cy=\"240\" r=\"25\" fill=\"#004000\" stroke=\"#003000\" stroke-width=\"2\">\n    <!-- Green light animation: off for 6s, on for 3s -->\n    <animate attributeName=\"fill\" values=\"#004000;#004000;#004000;#004000;#00FF00;#00FF00\" dur=\"9s\" repeatCount=\"indefinite\"></animate>\n    <animate attributeName=\"opacity\" values=\"0.3;0.3;0.3;0.3;1;1\" dur=\"9s\" repeatCount=\"indefinite\"></animate>\n  </circle>\n  \n  <!-- Light reflections/shine effects -->\n  <g xmlns=\"http://www.w3.org/2000/svg\" opacity=\"0.6\">\n    <!-- Red light shine -->\n    <ellipse cx=\"95\" cy=\"95\" rx=\"8\" ry=\"12\" fill=\"#FFFFFF\">\n      <animate attributeName=\"opacity\" values=\"0;0;0.6;0.6;0;0\" dur=\"9s\" repeatCount=\"indefinite\"></animate>\n    </ellipse>\n    \n    <!-- Yellow light shine -->\n    <ellipse cx=\"95\" cy=\"165\" rx=\"8\" ry=\"12\" fill=\"#FFFFFF\">\n      <animate attributeName=\"opacity\" values=\"0;0;0.6;0.6;0;0\" dur=\"9s\" repeatCount=\"indefinite\" begin=\"3s\"></animate>\n    </ellipse>\n    \n    <!-- Green light shine -->\n    <ellipse cx=\"95\" cy=\"235\" rx=\"8\" ry=\"12\" fill=\"#FFFFFF\">\n      <animate attributeName=\"opacity\" values=\"0;0;0.6;0.6;0;0\" dur=\"9s\" repeatCount=\"indefinite\" begin=\"6s\"></animate>\n    </ellipse>\n  </g>\n  \n  <!-- Housing bolts/screws -->\n  <circle xmlns=\"http://www.w3.org/2000/svg\" cx=\"65\" cy=\"70\" r=\"3\" fill=\"#555\"></circle>\n  <circle xmlns=\"http://www.w3.org/2000/svg\" cx=\"135\" cy=\"70\" r=\"3\" fill=\"#555\"></circle>\n  <circle xmlns=\"http://www.w3.org/2000/svg\" cx=\"65\" cy=\"270\" r=\"3\" fill=\"#555\"></circle>\n  <circle xmlns=\"http://www.w3.org/2000/svg\" cx=\"135\" cy=\"270\" r=\"3\" fill=\"#555\"></circle>\n  \n  <!-- Visor/hood for each light -->\n  <path xmlns=\"http://www.w3.org/2000/svg\" d=\"M70 85 Q100 75 130 85 L130 90 Q100 80 70 90 Z\" fill=\"#222\" opacit