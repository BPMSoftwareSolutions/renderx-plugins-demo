{
  "name": "@renderx-plugins/schema-fixer",
  "version": "0.1.0",
  "description": "Tool to discover, analyze, and migrate JSON files conforming to musical-sequence.schema.json to the latest version",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    }
  },
  "bin": {
    "schema-fixer": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest",
    "cli": "node dist/cli.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "ajv": "^8.12.0",
    "fast-glob": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
