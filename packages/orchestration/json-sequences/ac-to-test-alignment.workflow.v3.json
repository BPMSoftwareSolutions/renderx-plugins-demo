{
  "$schema": "https://json-schema.org/draft-07/schema",
  "musicalSequence": {
    "id": "ac-to-test-alignment-v3",
    "domainId": "renderx-web-orchestration",
    "title": "AC-to-Test Alignment: Analysis, Validation, and Conversion (v3)",
    "version": "3.2.0",
    "description": "Compute actual test counts, canonical AC-derived test counts, quality differences, and post-conversion projections; suggest and apply tags; validate test implementations match AC specifications; generate fixes for non-compliant tests.",
    "movements": [
      {
        "id": "movement-1",
        "title": "Discover & Normalize",
        "beats": [
          {
            "id": "beat-0",
            "title": "Generate Canonical Sequence Index",
            "handlers": [
              {
                "script": "scripts/renderx-web/generate-canonical-sequence-index.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "DOMAIN_REGISTRY.json"
                ],
                "when": [
                  "collect renderx-web and child domain sequences"
                ],
                "then": [
                  "emit canonical-sequences.manifest.json"
                ],
                "and": [
                  "include markdown summary under docs/generated/renderx-web-orchestration/"
                ]
              }
            ]
          },
          {
            "id": "beat-1",
            "title": "Generate AC Registry",
            "handlers": [
              {
                "script": "scripts/generate-ac-registry.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "domain registry is available"
                ],
                "when": [
                  "registry is processed"
                ],
                "then": [
                  "AC registry JSON is generated"
                ],
                "and": [
                  "invalid sequences are skipped gracefully"
                ]
              }
            ]
          },
          {
            "id": "beat-2",
            "title": "Discover Existing Tests",
            "handlers": [
              {
                "script": "scripts/analyze-tests.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "workspace root"
                ],
                "when": [
                  "glob search for spec/test files"
                ],
                "then": [
                  "return total count and per-package breakdown"
                ],
                "and": [
                  "exclude vendor and build outputs"
                ]
              }
            ]
          }
        ]
      },
      {
        "id": "movement-2",
        "title": "Canonical Mapping",
        "beats": [
          {
            "id": "beat-3",
            "title": "Compute Canonical Test Count",
            "handlers": [
              {
                "script": "scripts/ac-alignment/compute-canonical.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "AC registry"
                ],
                "when": [
                  "count ACs per beat"
                ],
                "then": [
                  "canonical minimum tests equals AC count"
                ],
                "and": [
                  "report total ACs and beats"
                ]
              }
            ]
          },
          {
            "id": "beat-4",
            "title": "Quality Difference Report",
            "handlers": [
              {
                "script": "scripts/ac-alignment/quality-diff.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "existing tests",
                  "AC registry"
                ],
                "when": [
                  "compare GWT structure and tags"
                ],
                "then": [
                  "list gaps: missing AC tags, missing THEN assertions, title mismatch"
                ],
                "and": [
                  "summarize per-domain and per-package"
                ]
              }
            ]
          }
        ]
      },
      {
        "id": "movement-3",
        "title": "Projection & Conversion",
        "beats": [
          {
            "id": "beat-5",
            "title": "Post-Conversion Projection",
            "handlers": [
              {
                "script": "scripts/ac-alignment/projection.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "existing test count",
                  "canonical count"
                ],
                "when": [
                  "apply tagging plan"
                ],
                "then": [
                  "estimate AC presence and THEN coverage"
                ],
                "and": [
                  "estimate number of tests after conversion"
                ]
              }
            ]
          },
          {
            "id": "beat-6a",
            "title": "Extract Handler Usage from Tests",
            "handlers": [
              {
                "script": "scripts/ac-alignment/extract-handler-usage.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "all test files"
                ],
                "when": [
                  "parse test content for handler imports and invocations"
                ],
                "then": [
                  "map each test to handlers it uses"
                ],
                "and": [
                  "output handler-to-test mapping"
                ]
              }
            ]
          },
          {
            "id": "beat-6b",
            "title": "Map Handlers to ACs",
            "handlers": [
              {
                "script": "scripts/ac-alignment/map-handlers-to-acs.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "AC registry with handlers",
                  "handler-to-test mapping"
                ],
                "when": [
                  "correlate handlers"
                ],
                "then": [
                  "generate comprehensive test-to-AC suggestions"
                ],
                "and": [
                  "every handler AC has at least one test"
                ]
              }
            ]
          },
          {
            "id": "beat-6c",
            "title": "Suggest Tags (Comprehensive)",
            "handlers": [
              {
                "script": "scripts/ac-alignment/suggest-tags-comprehensive.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "handler mapping",
                  "fuzzy matching",
                  "file patterns"
                ],
                "when": [
                  "combine all strategies"
                ],
                "then": [
                  "produce comprehensive suggestions.json"
                ],
                "and": [
                  "target 70%+ AC coverage"
                ]
              }
            ]
          },
          {
            "id": "beat-7",
            "title": "Apply Tags (Write to All Files)",
            "handlers": [
              {
                "script": "scripts/ac-alignment/apply-tags.cjs",
                "args": [
                  "--write"
                ]
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "comprehensive suggestions.json"
                ],
                "when": [
                  "apply tags in write mode"
                ],
                "then": [
                  "modify all relevant test files with AC/BEAT tags"
                ],
                "and": [
                  "all 127 AC handlers have tagged tests"
                ]
              }
            ]
          },
          {
            "id": "beat-8a",
            "title": "Verify Coverage (Scan Tags)",
            "handlers": [
              {
                "script": "scripts/ac-alignment/scan-existing-tags.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "all test files"
                ],
                "when": [
                  "scan for AC/BEAT tags"
                ],
                "then": [
                  "count tagged tests and coverage"
                ],
                "and": [
                  "report actual coverage percentage"
                ]
              }
            ]
          },
          {
            "id": "beat-8b",
            "title": "Validate Test Implementations",
            "handlers": [
              {
                "script": "scripts/ac-alignment/validate-test-implementations.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "tagged tests",
                  "AC registry"
                ],
                "when": [
                  "analyze test implementations"
                ],
                "then": [
                  "verify Given/When/Then conditions are properly implemented"
                ],
                "and": [
                  "report compliance rate and identify non-compliant tests"
                ]
              }
            ]
          },
          {
            "id": "beat-8c",
            "title": "Generate Test Implementation Fixes",
            "handlers": [
              {
                "script": "scripts/ac-alignment/fix-test-implementations.cjs"
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "validation results",
                  "AC registry"
                ],
                "when": [
                  "process non-compliant tests"
                ],
                "then": [
                  "generate suggested test implementations for each AC"
                ],
                "and": [
                  "output .fixes.txt files with Given/When/Then test code"
                ]
              }
            ]
          }
        ]
      },
      {
        "id": "movement-4",
        "title": "Artifacts & Thresholds",
        "beats": [
          {
            "id": "beat-8",
            "title": "Emit Analysis Artifacts",
            "handlers": [
              {
                "script": "scripts/ac-alignment/format-alignment-report.cjs",
                "args": [
                  "--domain",
                  "renderx-web-orchestration"
                ]
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "registry",
                  "suggestions"
                ],
                "when": [
                  "format report"
                ],
                "then": [
                  "write markdown + summary.json"
                ],
                "and": [
                  "include counts for existing, canonical, differences, projection",
                  "merge enhanced AC validator coverage from .generated/ac-alignment/validation-summary.json",
                  "ensure symphonic code analysis report reflects validator-based AC presence and THEN coverage"
                ]
              }
            ]
          },
          {
            "id": "beat-9",
            "title": "Gate with Thresholds",
            "handlers": [
              {
                "script": "scripts/ac-alignment/enforce-thresholds.cjs",
                "args": [
                  "--presence",
                  "0.3",
                  "--then",
                  "0.2"
                ]
              }
            ],
            "acceptanceCriteria": [
              {
                "given": [
                  "coverage metrics"
                ],
                "when": [
                  "check thresholds"
                ],
                "then": [
                  "emit pass/fail event"
                ],
                "and": [
                  "include actionable next steps"
                ]
              }
            ]
          }
        ]
      }
    ],
    "events": {
      "onStart": [
        "sequence:start"
      ],
      "onComplete": [
        "sequence:complete",
        "artifacts:ready"
      ],
      "onFailure": [
        "sequence:failed"
      ]
    },
    "artifacts": {
      "reports": [
        "docs/generated/renderx-web-orchestration/ac-alignment-report.md",
        ".generated/ac-alignment/summary.json",
        ".generated/ac-alignment/suggestions.json",
        ".generated/ac-alignment/patches.diff",
        "docs/generated/renderx-web-orchestration/test-analysis.md",
        "docs/generated/renderx-web-orchestration/ac-validation-report.md",
        ".generated/ac-alignment/validation-summary.json",
        "tests/**/*.fixes.txt",
        ".generated/analysis/renderx-web-orchestration/canonical-sequences.manifest.json",
        "docs/generated/renderx-web-orchestration/canonical-sequences.md"
      ]
    },
    "recommendedActions": [
      {
        "id": "rec-1",
        "title": "Confirm Handler Census (529 via code analysis)",
        "description": "Identify all 529 handlers in renderx-web orchestration via the symphonic code analysis pipeline; refine discovery tools if counts diverge.",
        "acceptanceCriteria": [
          "handler-exports.json (or equivalent) lists 529 handlers",
          "differences from prior counts are explained or tooling updated"
        ],
        "related": {
          "scripts": [
            "scripts/analyze-symphonic-code.cjs"
          ],
          "artifacts": [
            "docs/generated/renderx-web/renderx-web-orchestration-CODE-ANALYSIS-REPORT.md"
          ]
        }
      },
      {
        "id": "rec-2",
        "title": "Raise Coverage on HIGH-risk beats to ≥75%",
        "description": "Add runtime GWT assertions and telemetry timing checks to tests covering HIGH-risk beats until average coverage per beat is ≥75%.",
        "acceptanceCriteria": [
          "all beats marked HIGH-risk in Handler Symphonies reach ≥75% coverage",
          "tests include runtime Given/When/Then assertions and timing telemetry",
          "updated coverage reflected in code analysis report"
        ],
        "related": {
          "artifacts": [
            "docs/generated/renderx-web/renderx-web-orchestration-CODE-ANALYSIS-REPORT.md"
          ]
        }
      },
      {
        "id": "rec-3",
        "title": "Wire missing/aliased sequence files from canonical manifest",
        "description": "Resolve  missing or aliased sequence files listed by the canonical manifest to lift mapped handler coverage and beat counts.",
        "acceptanceCriteria": [
          "canonical-sequences.manifest.json shows 0 missing sequences or annotated deprecations",
          "mapped handler coverage increases and aligns with handler census"
        ],
        "related": {
          "artifacts": [
            ".generated/analysis/renderx-web-orchestration/canonical-sequences.manifest.json",
            "docs/generated/renderx-web-orchestration/canonical-sequences.md"
          ],
          "files": [
            "DOMAIN_REGISTRY.json",
            "orchestration-domains.json"
          ]
        }
      },
      {
        "id": "rec-4",
        "title": "Feed enhanced AC validator output into Movement 4",
        "description": "Ensure Movement 4 merges enhanced AC validator outputs so AC coverage in the published report reflects real status (presence and THEN checks).",
        "acceptanceCriteria": [
          "ac-alignment-report.md and summary.json include validator-based AC coverage",
          "symphonic report surfaces validator metrics for AC presence and THEN compliance"
        ],
        "related": {
          "artifacts": [
            "docs/generated/renderx-web-orchestration/ac-alignment-report.md",
            ".generated/ac-alignment/summary.json",
            ".generated/ac-alignment/validation-summary.json",
            "docs/generated/renderx-web/renderx-web-orchestration-CODE-ANALYSIS-REPORT.md"
          ]
        }
      },
      {
        "id": "rec-5",
        "title": "Reduce duplication hotspots (78.3%) via shared utilities",
        "description": "Extract shared DOM and encoder utilities to reduce duplication and lift maintainability index while lowering risk across symphonies.",
        "acceptanceCriteria": [
          "top duplication hotspots identified and refactored into shared modules",
          "measurable reduction in duplication and improvement in maintainability",
          "no regression in handler coverage or conformity"
        ],
        "related": {
          "artifacts": [
            "docs/generated/renderx-web/renderx-web-orchestration-CODE-ANALYSIS-REPORT.md"
          ]
        }
      }
    ]
  },
  "domainId": "renderx-web-orchestration"
}
