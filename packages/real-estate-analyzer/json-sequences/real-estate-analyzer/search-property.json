{
  "id": "real-estate-analyzer-search-symphony",
  "pluginId": "RealEstateAnalyzerPlugin",
  "name": "Real Estate Analyzer Search",
  "description": "Search for a property on Zillow and analyze its flipping potential",
  "movements": [
    {
      "id": "search",
      "name": "Search",
      "beats": [
        {
          "beat": 1,
          "event": "real.estate.analyzer.search",
          "handler": "fetchPropertyData",
          "kind": "pure",
          "dynamics": "mf",
          "timing": "immediate"
        },
        {
          "beat": 2,
          "event": "real.estate.analyzer:analyze",
          "handler": "analyze",
          "kind": "pure",
          "dynamics": "mf",
          "timing": "immediate"
        },
        {
          "beat": 3,
          "event": "real.estate.analyzer:format",
          "handler": "format",
          "kind": "pure",
          "dynamics": "mf",
          "timing": "immediate"
        }
      ]
    }
  ]
}

