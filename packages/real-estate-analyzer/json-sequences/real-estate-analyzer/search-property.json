{
  "id": "search-property",
  "name": "Search and Analyze Property",
  "description": "Search for a property on Zillow and analyze its flipping potential",
  "movements": [
    {
      "id": "fetch-property-data",
      "name": "Fetch Property Data",
      "beats": [
        {
          "id": "get-3d-tour",
          "name": "Get 3D Tour Data",
          "type": "handler",
          "handler": "fetchProperty3DTour",
          "input": {
            "propertyUrl": "$.propertyUrl"
          }
        }
      ]
    },
    {
      "id": "analyze-opportunity",
      "name": "Analyze Opportunity",
      "beats": [
        {
          "id": "calculate-scores",
          "name": "Calculate Opportunity Scores",
          "type": "handler",
          "handler": "analyzeProperty",
          "input": {
            "propertyData": "$.propertyData"
          }
        }
      ]
    },
    {
      "id": "return-results",
      "name": "Return Results",
      "beats": [
        {
          "id": "format-response",
          "name": "Format Response",
          "type": "handler",
          "handler": "formatResults",
          "input": {
            "analysis": "$.analysis"
          }
        }
      ]
    }
  ]
}

