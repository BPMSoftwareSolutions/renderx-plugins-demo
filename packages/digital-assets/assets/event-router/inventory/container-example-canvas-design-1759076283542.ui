        "preserveAspectRatio": "xMidYMid meet",
        "svgMarkup": "<svg viewBox=\"0 0 800 400\" xmlns=\"http://www.w3.org/2000/svg\">\n  <!-- Integrated: Depot background layer -->\n  <g id=\"depot-layer\">\n    <!-- Sky background -->\n    <rect x=\"0\" y=\"0\" width=\"800\" height=\"250\" fill=\"#87CEEB\"></rect>\n    \n    <!-- Clouds -->\n    <g fill=\"#FFFFFF\" opacity=\"0.8\">\n      <circle cx=\"150\" cy=\"60\" r=\"20\"></circle>\n      <circle cx=\"170\" cy=\"60\" r=\"15\"></circle>\n      <circle cx=\"130\" cy=\"65\" r=\"12\"></circle>\n      \n      <circle cx=\"600\" cy=\"80\" r=\"25\"></circle>\n      <circle cx=\"625\" cy=\"80\" r=\"18\"></circle>\n      <circle cx=\"580\" cy=\"85\" r=\"15\"></circle>\n    </g>\n    \n    <!-- Ground -->\n    <rect x=\"0\" y=\"250\" width=\"800\" height=\"150\" fill=\"#4A6B3A\"></rect>\n    \n    <!-- Depot building main structure -->\n    <rect x=\"100\" y=\"120\" width=\"300\" height=\"130\" fill=\"#8B7355\" stroke=\"#6B5535\" stroke-width=\"2\"></rect>\n    \n    <!-- Depot roof -->\n    <polygon points=\"80,120 220,80 420,80 420,120\" fill=\"#654321\" stroke=\"#543210\" stroke-width=\"2\"></polygon>\n    \n    <!-- Depot front overhang -->\n    <polygon points=\"90,120 90,110 410,110 410,120\" fill=\"#765432\" stroke=\"#654321\" stroke-width=\"1\"></polygon>\n    \n    <!-- Large garage doors -->\n    <rect x=\"120\" y=\"150\" width=\"80\" height=\"100\" fill=\"#666\" stroke=\"#444\" stroke-width=\"2\" rx=\"5\"></rect>\n    <rect x=\"220\" y=\"150\" width=\"80\" height=\"100\" fill=\"#666\" stroke=\"#444\" stroke-width=\"2\" rx=\"5\"></rect>\n    <rect x=\"320\" y=\"150\" width=\"60\" height=\"100\" fill=\"#666\" stroke=\"#444\" stroke-width=\"2\" rx=\"5\"></rect>\n    \n    <!-- Garage door details (panels) -->\n    <g stroke=\"#555\" stroke-width=\"1\" fill=\"none\">\n      <!-- Door 1 panels -->\n      <line x1=\"120\" y1=\"170\" x2=\"200\" y2=\"170\"></line>\n      <line x1=\"120\" y1=\"190\" x2=\"200\" y2=\"190\"></line>\n      <line x1=\"120\" y1=\"210\" x2=\"200\" y2=\"210\"></line>\n      <line x1=\"120\" y1=\"230\" x2=\"200\" y2=\"230\"></line>\n      \n      <!-- Door 2 panels -->\n      <line x1=\"220\" y1=\"170\" x2=\"300\" y2=\"170\"></line>\n      <line x1=\"220\" y1=\"190\" x2=\"300\" y2=\"190\"></line>\n      <line x1=\"220\" y1=\"210\" x2=\"300\" y2=\"210\"></line>\n      <line x1=\"220\" y1=\"230\" x2=\"300\" y2=\"230\"></line>\n      \n      <!-- Door 3 panels -->\n      <line x1=\"320\" y1=\"170\" x2=\"380\" y2=\"170\"></line>\n      <line x1=\"320\" y1=\"190\" x2=\"380\" y2=\"190\"></line>\n      <line x1=\"320\" y1=\"210\" x2=\"380\" y2=\"210\"></line>\n      <line x1=\"320\" y1=\"230\" x2=\"380\" y2=\"230\"></line>\n    </g>\n    \n    <!-- Garage door handles -->\n    <rect x=\"155\" y=\"198\" width=\"6\" height=\"4\" fill=\"#333\" rx=\"2\"></rect>\n    <rect x=\"255\" y=\"198\" width=\"6\" height=\"4\" fill=\"#333\" rx=\"2\"></rect>\n    <rect x=\"345\" y=\"198\" width=\"6\" height=\"4\" fill=\"#333\" rx=\"2\"></rect>\n    \n    <!-- Office entrance -->\n    <rect x=\"430\" y=\"180\" width=\"40\" height=\"70\" fill=\"#8B4513\" stroke=\"#654321\" stroke-width=\"2\" rx=\"3\"></rect>\n    <rect x=\"435\" y=\"185\" width=\"30\" height=\"35\" fill=\"#4A90E2\" opacity=\"0.7\" stroke=\"#2E5A8A\" stroke-width=\"1\"></rect>\n    <circle cx=\"465\" cy=\"215\" r=\"2\" fill=\"#FFD700\"></circle>\n    \n    <!-- Office windows -->\n    <rect x=\"480\" y=\"140\" width=\"40\" height=\"30\" fill=\"#4A90E2\" opacity=\"0.7\" stroke=\"#2E5A8A\" stroke-width=\"2\"></rect>\n    <rect x=\"530\" y=\"140\" width=\"40\" height=\"30\" fill=\"#4A90E2\" opacity=\"0.7\" stroke=\"#2E5A8A\" stroke-width=\"2\"></rect>\n    <line x1=\"500\" y1=\"140\" x2=\"500\" y2=\"170\" stroke=\"#2E5A8A\" stroke-width=\"1\"></line>\n    <line x1=\"480\" y1=\"155\" x2=\"520\" y2=\"155\" stroke=\"#2E5A8A\" stroke-width=\"1\"></line>\n    <line x1=\"550\" y1=\"140\" x2=\"550\" y2=\"170\" stroke=\"#2E5A8A\" stroke-width=\"1\"></line>\n    <line x1=\"530\" y1=\"155\" x2=\"570\" y2=\"155\" stroke=\"#2E5A8A\" stroke-width=\"1\"></line>\n    \n    <!-- Sign on building -->\n    <rect x=\"200\" y=\"95\" width=\"120\" height=\"20\" fill=\"#FFFFFF\" stroke=\"#333\" stroke-width=\"1\" rx=\"3\"></rect>\n    <text x=\"260\" y=\"108\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"12\" font-weight=\"bold\" fill=\"#333\">EVENT BUS DEPOT</text>\n\n    <!-- Paved area/parking lot (road) -->\n    <rect x=\"0\" y=\"250\" width=\"800\" height=\"50\" fill=\"#555\"></rect>\n    \n    <!-- Parking lot lines -->\n    <g stroke=\"#FFFF00\" stroke-width=\"2\" fill=\"none\">\n      <line x1=\"50\" y1=\"250\" x2=\"50\" y2=\"300\"></line>\n      <line x1=\"150\" y1=\"250\" x2=\"150\" y2=\"300\"></line>\n      <line x1=\"250\" y1=\"250\" x2=\"250\" y2=\"300\"></line>\n      <line x1=\"350\" y1=\"250\" x2=\"350\" y2=\"300\"></line>\n      <line x1=\"450\" y1=\"250\" x2=\"450\" y2=\"300\"></line>\n      <line x1=\"550\" y1=\"250\" x2=\"550\" y2=\"300\"></line>\n      <line x1=\"650\" y1=\"250\" x2=\"650\" y2=\"300\"></line>\n      <line x1=\"750\" y1=\"250\" x2=\"750\" y2=\"300\"></line>\n    </g>\n    \n    <!-- Fuel station -->\n    <rect x=\"600\" y=\"180\" width=\"80\" height=\"70\" fill=\"#C0C0C0\" stroke=\"#A0A0A0\" stroke-width=\"2\"></rect>\n    <rect x=\"610\" y=\"160\" width=\"60\" height=\"20\" fill=\"#FF0000\" stroke=\"#CC0000\" stroke-width=\"1\"></rect>\n    <text x=\"640\" y=\"173\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"10\" font-weight=\"bold\" fill=\"#FFF\">FUEL</text>\n    \n    <!-- Fuel pump -->\n    <rect x=\"630\" y=\"190\" width=\"20\" height=\"40\" fill=\"#333\" stroke=\"#222\" stroke-width=\"1\" rx=\"3\"></rect>\n    <circle cx=\"640\" cy=\"200\" r=\"3\" fill=\"#00FF00\"></circle>\n    <circle cx=\"640\" cy=\"210\" r=\"3\" fill=\"#FF0000\"></circle>\n    <rect x=\"635\" y=\"220\" width=\"10\" height=\"8\" fill=\"#666\" rx=\"2\"></rect>\n    \n    <!-- Fuel hose -->\n    <path d=\"M645,225 Q655,230 660,240 Q665,245 670,250\" stroke=\"#000\" stroke-width=\"3\" fill=\"none\"></path>\n    \n    <!-- Trees around the depot -->\n    <g fill=\"#228B22\">\n      <!-- Tree 1 -->\n      <rect x=\"25\" y=\"220\" width=\"8\" height=\"30\" fill=\"#8B4513\"></rect>\n      <circle cx=\"29\" cy=\"215\" r=\"15\" fill=\"#228B22\"></circle>\n      \n      <!-- Tree 2 -->\n      <rect x=\"720\" y=\"210\" width=\"10\" height=\"40\" fill=\"#8B4513\"></rect>\n      <circle cx=\"725\" cy=\"205\" r=\"18\" fill=\"#228B22\"></circle>\n      \n      <!-- Tree 3 -->\n      <rect x=\"650\" y=\"230\" width=\"6\" height=\"20\" fill=\"#8B4513\"></rect>\n      <circle cx=\"653\" cy=\"225\" r=\"12\" fill=\"#228B22\"></circle>\n    </g>\n    \n    <!-- Fence along property -->\n    <g stroke=\"#666\" stroke-width=\"2\" fill=\"none\">\n      <line x1=\"0\" y1=\"300\" x2=\"100\" y2=\"300\"></line>\n      <line x1=\"700\" y1=\"300\" x2=\"800\" y2=\"300\"></line>\n      <!-- Fence posts -->\n      <line x1=\"20\" y1=\"295\" x2=\"20\" y2=\"305\"></line>\n      <line x1=\"40\" y1=\"295\" x2=\"40\" y2=\"305\"></line>\n      <line x1=\"60\" y1=\"295\" x2=\"60\" y2=\"305\"></line>\n      <line x1=\"80\" y1=\"295\" x2=\"80\" y2=\"305\"></line>\n      <line x1=\"720\" y1=\"295\" x2=\"720\" y2=\"305\"></line>\n      <line x1=\"740\" y1=\"295\" x2=\"740\" y2=\"305\"></line>\n      <line x1=\"760\" y1=\"295\" x2=\"760\" y2=\"305\"></line>\n      <line x1=\"780\" y1=\"295\" x2=\"780\" y2=\"305\"></line>\n    </g>\n    \n    <!-- Bus washing bay -->\n    <rect x=\"500\" y=\"200\" width=\"60\" height=\"50\" fill=\"#E6E6FA\" stroke=\"#D3D3D3\" stroke-width=\"2\" opacity=\"0.8\"></rect>\n    <text x=\"530\" y=\"230\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"8\" fill=\"#666\">WASH BAY</text>\n    \n    <!-- Maintenance worker -->\n    <g fill=\"#FF6600\">\n      <circle cx=\"320\" cy=\"240\" r=\"4\" fill=\"#FFDBAC\"></circle> <!-- head -->\n      <rect x=\"318\" y=\"244\" width=\"4\" height=\"8\" fill=\"#FF6600\"></rect> <!-- body -->\n      <rect x=\"316\" y=\"248\" width=\"2\" height=\"6\" fill=\"#0066CC\"></rect> <!-- legs -->\n      <rect x=\"322\" y=\"248\" width=\"2\" height=\"6\" fill=\"#0066CC\"></rect>\n      <rect x=\"314\" y=\"246\" width=\"3\" height=\"1\" fill=\"#FFDBAC\"></rect> <!-- arm -->\n      <rect x=\"323\" y=\"246\" width=\"3\" height=\"1\" fill=\"#FFDBAC\"></rect>\n    </g>\n  </g>\n\n  <!-- Integrated: Bus foreground layer, vertically aligned via mapping (translateY=125) -->\n  <g id=\"bus-layer\" transform=\"translate(0, 125)\">\n    <!-- Bus movement animation group (horizontal) -->\n    <g>\n      <animateTransform attributeName=\"transform\" type=\"translate\" values=\"-365,0;1000,0\" dur=\"20s\" repeatCount=\"indefinite\"></animateTransform>\n      \n      <!-- Bus body -->\n      <rect x=\"70\" y=\"60\" width=\"280\" height=\"80\" rx=\"8\" ry=\"8\" fill=\"#FFD700\" stroke=\"#E6C200\" stroke-width=\"2\"></rect>\n    \n      <!-- Bus front -->\n      <rect x=\"340\" y=\"70\" width=\"20\" height=\"60\" rx=\"10\" ry=\"10\" fill=\"#FFD700\" stroke=\"#E6C200\" stroke-width=\"2\"></rect>\n      \n      <!-- Bus roof -->\n      <rect x=\"65\" y=\"55\" width=\"290\" height=\"10\" rx=\"5\" ry=\"5\" fill=\"#FFA500\"></rect>\n      \n      <!-- Front bumper -->\n      <rect x=\"350\" y=\"125\" width=\"15\" height=\"8\" rx=\"4\" ry=\"4\" fill=\"#C0C0C0\"></rect>\n      \n      <!-- Windows (tinted dark) -->\n      <!-- Front windshield -->\n      <rect x=\"342\" y=\"75\" width=\"16\" height=\"20\" rx=\"2\" ry=\"2\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n      \n      <!-- Side windows -->\n      <rect x=\"305\" y=\"70\" width=\"25\" height=\"20\" rx=\"2\" ry=\"2\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n      <rect x=\"275\" y=\"70\" width=\"25\" height=\"20\" rx=\"2\" ry=\"2\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n      <rect x=\"245\" y=\"70\" width=\"25\" height=\"20\" rx=\"2\" ry=\"2\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n      <rect x=\"215\" y=\"70\" width=\"25\" height=\"20\" rx=\"2\" ry=\"2\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n      <rect x=\"185\" y=\"70\" width=\"25\" height=\"20\" rx=\"2\" ry=\"2\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n      <rect x=\"155\" y=\"70\" width=\"25\" height=\"20\" rx=\"2\" ry=\"2\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n      <rect x=\"125\" y=\"70\" width=\"25\" height=\"20\" rx=\"2\" ry=\"2\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n      <rect x=\"95\" y=\"70\" width=\"25\" height=\"20\" rx=\"2\" ry=\"2\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n      \n      <!-- Emergency exit window -->\n      <rect x=\"75\" y=\"75\" width=\"15\" height=\"15\" rx=\"2\" ry=\"2\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n      \n      <!-- Door -->\n      <rect x=\"315\" y=\"95\" width=\"20\" height=\"35\" rx=\"2\" ry=\"2\" fill=\"#E6C200\" stroke=\"#D4AF37\" stroke-width=\"1\"></rect>\n      <rect x=\"317\" y=\"97\" width=\"16\" height=\"15\" rx=\"1\" ry=\"1\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n      <circle cx=\"319\" cy=\"112\" r=\"1.5\" fill=\"#000\"></circle>\n      \n      <!-- School Bus text -->\n      <text x=\"200\" y=\"110\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"14\" font-weight=\"bold\" fill=\"#000\">EVENT BUS</text>\n\n      <!-- Front and rear lights -->\n      <circle cx=\"353\" cy=\"80\" r=\"3\" fill=\"#FFFF00\"></circle>\n      <circle cx=\"353\" cy=\"120\" r=\"3\" fill=\"#FF4500\"></circle>\n      <rect x=\"69\" y=\"77\" width=\"6\" height=\"8\" rx=\"3\" ry=\"3\" fill=\"#FF0000\"></rect>\n      <rect x=\"69\" y=\"110\" width=\"6\" height=\"8\" rx=\"3\" ry=\"3\" fill=\"#FF0000\"></rect>\n      \n      <!-- Wheels (static geometry; visual spin could be added via inner animate) -->\n      <g>\n        <!-- Front wheel -->\n        <circle cx=\"310\" cy=\"155\" r=\"20\" fill=\"#2F2F2F\" stroke=\"#000\" stroke-width=\"2\"></circle>\n        <circle cx=\"310\" cy=\"155\" r=\"15\" fill=\"#4F4F4F\"></circle>\n        <circle cx=\"310\" cy=\"155\" r=\"8\" fill=\"#696969\"></circle>\n        <circle cx=\"310\" cy=\"155\" r=\"3\" fill=\"#2F2F2F\"></circle>\n        <!-- Rear wheel -->\n        <circle cx=\"110\" cy=\"155\" r=\"20\" fill=\"#2F2F2F\" stroke=\"#000\" stroke-width=\"2\"></circle>\n        <circle cx=\"110\" cy=\"155\" r=\"15\" fill=\"#4F4F4F\"></circle>\n        <circle cx=\"110\" cy=\"155\" r=\"8\" fill=\"#696969\"></circle>\n        <circle cx=\"110\" cy=\"155\" r=\"3\" fill=\"#2F2F2F\"></circle>\n      </g>\n    </g>\n  </g>\n</svg>\n\n"
      }
    }
  ]
