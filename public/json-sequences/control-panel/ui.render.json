{
  "pluginId": "ControlPanelPlugin",
  "id": "control-panel-ui-render-symphony",
  "name": "Control Panel UI Render",
  "description": "Generate and render Control Panel UI fields and sections",
  "movements": [
    {
      "id": "rendering",
      "name": "UI Rendering",
      "beats": [
        {
          "beat": 1,
          "event": "control:panel:ui:fields:generate",
          "title": "Generate Property Fields",
          "dynamics": "mf",
          "handler": "generateFields",
          "timing": "immediate",
          "kind": "stage-crew"
        },
        {
          "beat": 2,
          "event": "control:panel:ui:sections:generate",
          "title": "Generate Sections",
          "dynamics": "mf",
          "handler": "generateSections",
          "timing": "immediate",
          "kind": "stage-crew"
        },
        {
          "beat": 3,
          "event": "control:panel:ui:view:render",
          "title": "Render UI View",
          "dynamics": "f",
          "handler": "renderView",
          "timing": "immediate",
          "kind": "pure"
        }
      ]
    }
  ]
}
