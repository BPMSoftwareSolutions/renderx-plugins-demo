{
  "pluginId": "CanvasComponentImportPlugin",
  "id": "canvas-component-import-symphony",
  "name": "Canvas Component Import",
  "movements": [
    {
      "id": "import",
      "name": "Import",
      "beats": [
        {
          "beat": 1,
          "event": "canvas:component:ui:open",
          "title": "Open UI File",
          "handler": "openUiFile",
          "timing": "immediate",
          "dynamics": "mf",
          "kind": "stage-crew",
          "handlerDef": {
            "id": "canvas-component-import-symphony.openuifile.0",
            "scope": "plugin",
            "kind": "plugin"
          }
        },
        {
          "beat": 2,
          "event": "canvas:component:ui:parse",
          "title": "Parse UI File",
          "handler": "parseUiFile",
          "timing": "immediate",
          "dynamics": "mf",
          "kind": "pure",
          "handlerDef": {
            "id": "canvas-component-import-symphony.parseuifile.1",
            "scope": "plugin",
            "kind": "plugin"
          }
        },
        {
          "beat": 3,
          "event": "canvas:component:css:inject",
          "title": "Inject CSS Classes",
          "handler": "injectCssClasses",
          "timing": "immediate",
          "dynamics": "mf",
          "kind": "stage-crew",
          "handlerDef": {
            "id": "canvas-component-import-symphony.injectcssclasses.2",
            "scope": "plugin",
            "kind": "plugin"
          }
        },
        {
          "beat": 4,
          "event": "canvas:component:nodes:create",
          "title": "Create Components Sequentially",
          "handler": "createComponentsSequentially",
          "timing": "immediate",
          "dynamics": "mf",
          "kind": "stage-crew",
          "handlerDef": {
            "id": "canvas-component-import-symphony.createcomponentssequentially.3",
            "scope": "plugin",
            "kind": "plugin"
          }
        },
        {
          "beat": 5,
          "event": "canvas:component:hierarchy:apply",
          "title": "Apply Hierarchy & Order",
          "handler": "applyHierarchyAndOrder",
          "timing": "immediate",
          "dynamics": "mf",
          "kind": "stage-crew",
          "handlerDef": {
            "id": "canvas-component-import-symphony.applyhierarchyandorder.4",
            "scope": "plugin",
            "kind": "plugin"
          }
        }
      ]
    }
  ]
}
