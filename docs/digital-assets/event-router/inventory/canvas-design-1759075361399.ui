        "preserveAspectRatio": "xMidYMid meet",
        "svgMarkup": "<svg viewBox=\"0 0 800 600\" xmlns=\"http://www.w3.org/2000/svg\">\n  <!-- Sky background -->\n  <rect x=\"0\" y=\"0\" width=\"800\" height=\"400\" fill=\"#87CEEB\"></rect>\n  \n  <!-- Clouds -->\n  <g fill=\"#FFFFFF\" opacity=\"0.8\">\n    <circle cx=\"150\" cy=\"80\" r=\"25\"></circle>\n    <circle cx=\"175\" cy=\"80\" r=\"20\"></circle>\n    <circle cx=\"125\" cy=\"85\" r=\"15\"></circle>\n    \n    <circle cx=\"650\" cy=\"100\" r=\"30\"></circle>\n    <circle cx=\"680\" cy=\"100\" r=\"22\"></circle>\n    <circle cx=\"620\" cy=\"105\" r=\"18\"></circle>\n  </g>\n  \n  <!-- Ground -->\n  <rect x=\"0\" y=\"400\" width=\"800\" height=\"200\" fill=\"#4A6B3A\"></rect>\n  \n  <!-- Main hub building -->\n  <rect x=\"250\" y=\"200\" width=\"300\" height=\"200\" fill=\"#C0C0C0\" stroke=\"#A0A0A0\" stroke-width=\"3\"></rect>\n  \n  <!-- Hub roof -->\n  <polygon points=\"230,200 400,150 570,200\" fill=\"#654321\" stroke=\"#543210\" stroke-width=\"2\"></polygon>\n  \n  <!-- Large windows -->\n  <rect x=\"270\" y=\"220\" width=\"60\" height=\"80\" fill=\"#4A90E2\" opacity=\"0.7\" stroke=\"#2E5A8A\" stroke-width=\"2\"></rect>\n  <rect x=\"350\" y=\"220\" width=\"60\" height=\"80\" fill=\"#4A90E2\" opacity=\"0.7\" stroke=\"#2E5A8A\" stroke-width=\"2\"></rect>\n  <rect x=\"430\" y=\"220\" width=\"60\" height=\"80\" fill=\"#4A90E2\" opacity=\"0.7\" stroke=\"#2E5A8A\" stroke-width=\"2\"></rect>\n  <rect x=\"510\" y=\"220\" width=\"60\" height=\"80\" fill=\"#4A90E2\" opacity=\"0.7\" stroke=\"#2E5A8A\" stroke-width=\"2\"></rect>\n  \n  <!-- Window frames -->\n  <g stroke=\"#2E5A8A\" stroke-width=\"1\" fill=\"none\">\n    <line x1=\"300\" y1=\"220\" x2=\"300\" y2=\"300\"></line>\n    <line x1=\"270\" y1=\"260\" x2=\"330\" y2=\"260\"></line>\n    <line x1=\"380\" y1=\"220\" x2=\"380\" y2=\"300\"></line>\n    <line x1=\"350\" y1=\"260\" x2=\"410\" y2=\"260\"></line>\n    <line x1=\"460\" y1=\"220\" x2=\"460\" y2=\"300\"></line>\n    <line x1=\"430\" y1=\"260\" x2=\"490\" y2=\"260\"></line>\n    <line x1=\"540\" y1=\"220\" x2=\"540\" y2=\"300\"></line>\n    <line x1=\"510\" y1=\"260\" x2=\"570\" y2=\"260\"></line>\n  </g>\n  \n  <!-- Main entrance -->\n  <rect x=\"360\" y=\"320\" width=\"80\" height=\"80\" fill=\"#8B4513\" stroke=\"#654321\" stroke-width=\"2\" rx=\"5\"></rect>\n  <rect x=\"370\" y=\"330\" width=\"60\" height=\"40\" fill=\"#4A90E2\" opacity=\"0.7\" stroke=\"#2E5A8A\" stroke-width=\"2\"></rect>\n  <circle cx=\"425\" cy=\"365\" r=\"3\" fill=\"#FFD700\"></circle>\n  \n  <!-- Hub sign -->\n  <rect x=\"330\" y=\"170\" width=\"140\" height=\"25\" fill=\"#FFFFFF\" stroke=\"#333\" stroke-width=\"2\" rx=\"5\"></rect>\n  <text x=\"400\" y=\"187\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"16\" font-weight=\"bold\" fill=\"#333\">CONDUCTOR HUB</text>\n  \n  <!-- Control tower -->\n  <rect x=\"480\" y=\"140\" width=\"40\" height=\"60\" fill=\"#B0B0B0\" stroke=\"#909090\" stroke-width=\"2\"></rect>\n  <rect x=\"485\" y=\"120\" width=\"30\" height=\"20\" fill=\"#333\" stroke=\"#222\" stroke-width=\"1\"></rect>\n  <rect x=\"490\" y=\"125\" width=\"20\" height=\"10\" fill=\"#FF0000\"></rect>\n  <!-- Antenna -->\n  <line x1=\"500\" y1=\"120\" x2=\"500\" y2=\"100\" stroke=\"#666\" stroke-width=\"2\"></line>\n  <circle cx=\"500\" cy=\"100\" r=\"3\" fill=\"#FF0000\"></circle>\n  \n  <!-- Platform areas -->\n  <rect x=\"100\" y=\"380\" width=\"150\" height=\"20\" fill=\"#555\"></rect>\n  <rect x=\"550\" y=\"380\" width=\"150\" height=\"20\" fill=\"#555\"></rect>\n  <rect x=\"350\" y=\"450\" width=\"100\" height=\"20\" fill=\"#555\"></rect>\n  \n  <!-- Platform shelters -->\n  <rect x=\"120\" y=\"340\" width=\"110\" height=\"40\" fill=\"#C0C0C0\" stroke=\"#A0A0A0\" stroke-width=\"2\" opacity=\"0.8\"></rect>\n  <rect x=\"570\" y=\"340\" width=\"110\" height=\"40\" fill=\"#C0C0C0\" stroke=\"#A0A0A0\" stroke-width=\"2\" opacity=\"0.8\"></rect>\n  \n  <!-- Route indicator boards -->\n  <rect x=\"125\" y=\"350\" width=\"40\" height=\"15\" fill=\"#000\"></rect>\n  <text x=\"145\" y=\"361\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"8\" fill=\"#00FF00\">ROUTE A</text>\n  <rect x=\"575\" y=\"350\" width=\"40\" height=\"15\" fill=\"#000\"></rect>\n  <text x=\"595\" y=\"361\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"8\" fill=\"#00FF00\">ROUTE B</text>\n  \n  <!-- Underground tunnels (entrances) -->\n  <ellipse cx=\"200\" cy=\"420\" rx=\"25\" ry=\"15\" fill=\"#333\"></ellipse>\n  <rect x=\"190\" y=\"415\" width=\"20\" height=\"10\" fill=\"#666\"></rect>\n  <text x=\"200\" y=\"440\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"8\" fill=\"#666\">TUNNEL</text>\n  \n  <ellipse cx=\"600\" cy=\"420\" rx=\"25\" ry=\"15\" fill=\"#333\"></ellipse>\n  <rect x=\"590\" y=\"415\" width=\"20\" height=\"10\" fill=\"#666\"></rect>\n  <text x=\"600\" y=\"440\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"8\" fill=\"#666\">TUNNEL</text>\n  \n  <!-- Multiple road connections -->\n  <!-- Road from left -->\n  <polygon points=\"0,450 200,450 200,470 0,470\" fill=\"#404040\"></polygon>\n  <line x1=\"0\" y1=\"460\" x2=\"200\" y2=\"460\" stroke=\"#FFFF00\" stroke-width=\"2\" stroke-dasharray=\"20,10\"></line>\n  \n  <!-- Road from right -->\n  <polygon points=\"600,450 800,450 800,470 600,470\" fill=\"#404040\"></polygon>\n  <line x1=\"600\" y1=\"460\" x2=\"800\" y2=\"460\" stroke=\"#FFFF00\" stroke-width=\"2\" stroke-dasharray=\"20,10\"></line>\n  \n  <!-- Road from bottom -->\n  <polygon points=\"380,470 420,470 420,600 380,600\" fill=\"#404040\"></polygon>\n  <line x1=\"400\" y1=\"470\" x2=\"400\" y2=\"600\" stroke=\"#FFFF00\" stroke-width=\"2\" stroke-dasharray=\"20,10\"></line>\n  \n  <!-- Communication arrays -->\n  <g stroke=\"#666\" stroke-width=\"2\" fill=\"none\">\n    <!-- Satellite dishes -->\n    <ellipse cx=\"300\" cy=\"180\" rx=\"15\" ry=\"8\" transform=\"rotate(-20 300 180)\"></ellipse>\n    <line x1=\"300\" y1=\"188\" x2=\"300\" y2=\"200\"></line>\n    \n    <ellipse cx=\"500\" cy=\"180\" rx=\"15\" ry=\"8\" transform=\"rotate(20 500 180)\"></ellipse>\n    <line x1=\"500\" y1=\"188\" x2=\"500\" y2=\"200\"></line>\n  </g>\n  \n  <!-- Traffic management center -->\n  <rect x=\"280\" y=\"310\" width=\"40\" height=\"20\" fill=\"#222\"></rect>\n  <g fill=\"#00FF00\" opacity=\"0.8\">\n    <rect x=\"285\" y=\"315\" width=\"3\" height=\"3\"></rect>\n    <rect x=\"290\" y=\"315\" width=\"3\" height=\"3\"></rect>\n    <rect x=\"295\" y=\"315\" width=\"3\" height=\"3\"></rect>\n    <rect x=\"300\" y=\"315\" width=\"3\" height=\"3\"></rect>\n    <rect x=\"305\" y=\"315\" width=\"3\" height=\"3\"></rect>\n    <rect x=\"310\" y=\"315\" width=\"3\" height=\"3\"></rect>\n    \n    <rect x=\"285\" y=\"320\" width=\"3\" height=\"3\"></rect>\n    <rect x=\"295\" y=\"320\" width=\"3\" height=\"3\"></rect>\n    <rect x=\"305\" y=\"320\" width=\"3\" height=\"3\"></rect>\n    <rect x=\"315\" y=\"320\" width=\"3\" height=\"3\"></rect>\n  </g>\n  <text x=\"300\" y=\"340\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"6\" fill=\"#666\">CONTROL</text>\n  \n  <!-- Parking areas for conductor vehicles -->\n  <g stroke=\"#FFFF00\" stroke-width=\"1\" fill=\"none\">\n    <!-- Parking spaces -->\n    <rect x=\"50\" y=\"500\" width=\"30\" height=\"15\"></rect>\n    <rect x=\"85\" y=\"500\" width=\"30\" height=\"15\"></rect>\n    <rect x=\"120\" y=\"500\" width=\"30\" height=\"15\"></rect>\n    \n    <rect x=\"650\" y=\"500\" width=\"30\" height=\"15\"></rect>\n    <rect x=\"685\" y=\"500\" width=\"30\" height=\"15\"></rect>\n    <rect x=\"720\" y=\"500\" width=\"30\" height=\"15\"></rect>\n  </g>\n  \n  <!-- Conductor vehicles (small) -->\n  <rect x=\"52\" y=\"502\" width=\"26\" height=\"11\" fill=\"#FFD700\" rx=\"2\"></rect>\n  <circle cx=\"58\" cy=\"511\" r=\"2\" fill=\"#333\"></circle>\n  <circle cx=\"74\" cy=\"511\" r=\"2\" fill=\"#333\"></circle>\n  <te