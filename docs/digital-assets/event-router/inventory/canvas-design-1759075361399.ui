        "content": "CONDUCTOR HUB\n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  ROUTE A\n  \n  ROUTE B\n  \n  \n  \n  \n  TUNNEL\n  \n  \n  \n  TUNNEL\n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n    \n    \n    \n    \n    \n    \n  \n  \n  \n  \n  \n    \n    \n    \n    \n    \n    \n    \n    \n    \n    \n    \n  \n  CONTROL\n  \n  \n  \n    \n    \n    \n    \n    \n    \n    \n    \n  \n  \n  \n  \n  \n  \n  CONDUCTOR\n  \n  \n  \n  \n  CONDUCTOR\n  \n  \n  \n  \n  INFO\n  \n  \n  \n  INFO",
        "text": "CONDUCTOR HUB\n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  ROUTE A\n  \n  ROUTE B\n  \n  \n  \n  \n  TUNNEL\n  \n  \n  \n  TUNNEL\n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n  \n    \n    \n    \n    \n    \n    \n  \n  \n  \n  \n  \n    \n    \n    \n    \n    \n    \n    \n    \n    \n    \n    \n  \n  CONTROL\n  \n  \n  \n    \n    \n    \n    \n    \n    \n    \n    \n  \n  \n  \n  \n  \n  \n  CONDUCTOR\n  \n  \n  \n  \n  CONDUCTOR\n  \n  \n  \n  \n  INFO\n  \n  \n  \n  INFO",
