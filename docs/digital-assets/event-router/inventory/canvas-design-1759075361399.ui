        "svgMarkup": "<svg viewBox=\"0 0 800 400\" xmlns=\"http://www.w3.org/2000/svg\">\n  <!-- Integrated: Depot background layer -->\n  <g id=\"depot-layer\">\n    <!-- Sky background -->\n    <rect x=\"0\" y=\"0\" width=\"800\" height=\"250\" fill=\"#87CEEB\"></rect>\n    \n    <!-- Clouds -->\n    <g fill=\"#FFFFFF\" opacity=\"0.8\">\n      <circle cx=\"150\" cy=\"60\" r=\"20\"></circle>\n      <circle cx=\"170\" cy=\"60\" r=\"15\"></circle>\n      <circle cx=\"130\" cy=\"65\" r=\"12\"></circle>\n      \n      <circle cx=\"600\" cy=\"80\" r=\"25\"></circle>\n      <circle cx=\"625\" cy=\"80\" r=\"18\"></circle>\n      <circle cx=\"580\" cy=\"85\" r=\"15\"></circle>\n    </g>\n    \n    <!-- Ground -->\n    <rect x=\"0\" y=\"250\" width=\"800\" height=\"150\" fill=\"#4A6B3A\"></rect>\n    \n    <!-- Depot building main structure -->\n    <rect x=\"100\" y=\"120\" width=\"300\" height=\"130\" fill=\"#8B7355\" stroke=\"#6B5535\" stroke-width=\"2\"></rect>\n    \n    <!-- Depot roof -->\n    <polygon points=\"80,120 220,80 420,80 420,120\" fill=\"#654321\" stroke=\"#543210\" stroke-width=\"2\"></polygon>\n    \n    <!-- Depot front overhang -->\n    <polygon points=\"90,120 90,110 410,110 410,120\" fill=\"#765432\" stroke=\"#654321\" stroke-width=\"1\"></polygon>\n    \n    <!-- Large garage doors -->\n    <rect x=\"120\" y=\"150\" width=\"80\" height=\"100\" fill=\"#666\" stroke=\"#444\" stroke-width=\"2\" rx=\"5\"></rect>\n    <rect x=\"220\" y=\"150\" width=\"80\" height=\"100\" fill=\"#666\" stroke=\"#444\" stroke-width=\"2\" rx=\"5\"></rect>\n    <rect x=\"320\" y=\"150\" width=\"60\" height=\"100\" fill=\"#666\" stroke=\"#444\" stroke-width=\"2\" rx=\"5\"></rect>\n    \n    <!-- Garage door details (panels) -->\n    <g stroke=\"#555\" stroke-width=\"1\" fill=\"none\">\n      <!-- Door 1 panels -->\n      <line x1=\"120\" y1=\"170\" x2=\"200\" y2=\"170\"></line>\n      <line x1=\"120\" y1=\"190\" x2=\"200\" y2=\"190\"></line>\n      <line x1=\"120\" y1=\"210\"