\n  <!-- Emergency exit window -->\n  <rect x=\"75\" y=\"75\" width=\"15\" height=\"15\" rx=\"2\" ry=\"2\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n  \n  <!-- Door -->\n  <rect x=\"315\" y=\"95\" width=\"20\" height=\"35\" rx=\"2\" ry=\"2\" fill=\"#E6C200\" stroke=\"#D4AF37\" stroke-width=\"1\"></rect>\n  <rect x=\"317\" y=\"97\" width=\"16\" height=\"15\" rx=\"1\" ry=\"1\" fill=\"#2F4F4F\" opacity=\"0.8\"></rect>\n  <circle cx=\"319\" cy=\"112\" r=\"1.5\" fill=\"#000\"></circle>\n  \n  <!-- School Bus text -->\n  <text x=\"200\" y=\"110\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"14\" font-weight=\"bold\" fill=\"#000\">SCHOOL BUS</text>\n  \n  <!-- Front and rear lights -->\n  <circle cx=\"353\" cy=\"80\" r=\"3\" fill=\"#FFFF00\"></circle>\n  <circle cx=\"353\" cy=\"120\" r=\"3\" fill=\"#FF4500\"></circle>\n  <rect x=\"69\" y=\"77\" width=\"6\" height=\"8\" rx=\"3\" ry=\"3\" fill=\"#FF0000\"></rect>\n  <rect x=\"69\" y=\"110\" width=\"6\" height=\"8\" rx=\"3\" ry=\"3\" fill=\"#FF0000\"></rect>\n  \n  <!-- Wheels with spinning animation -->\n  <g>\n    <!-- Front wheel (now on the right) -->\n    <circle cx=\"310\" cy=\"155\" r=\"20\" fill=\"#2F2F2F\" stroke=\"#000\" stroke-width=\"2\"></circle>\n    <circle cx=\"310\" cy=\"155\" r=\"15\" fill=\"#4F4F4F\"></circle>\n    <circle cx=\"310\" cy=\"155\" r=\"8\" fill=\"#696969\"></circle>\n    <circle cx=\"310\" cy=\"155\" r=\"3\" fill=\"#2F2F2F\"></circle>\n    <!-- Animated wheel spokes -->\n    <g>\n      <animateTransform attributeName=\"transform\" type=\"rotate\" values=\"0 310 155;360 310 155\" dur=\"1s\" repeatCount=\"indefinite\"></animateTransform>\n      <line x1=\"310\" y1=\"140\" x2=\"310\" y2=\"170\" stroke=\"#A9A9A9\" stroke-width=\"2\"></line>\n      <line x1=\"325\" y1=\"155\" x2=\"295\" y2=\"155\" stroke=\"#A9A9A9\" stroke-width=\"2\"></line>\n      <line x1=\"321\" y1=\"144\" x2=\"299\" y2=\"166\" stroke=\"#A9A9A9\" stroke-width=\"2\"></line>\n      <line x1=\"321\" y1=\"166\" x2=\"299\" y2=\"144\" stroke=\"#A9A9A9\" stroke-width=\"2\"></line>\n    </g>\n    \n    <!-- Rear wheel (now on the left) -->\n    <circle cx=\"110\" cy=\"155\" r=\"20\" fill=\"#2F2F2F\" stroke=\"#000\" stroke-width=\"2\"></circle>\n    <circle cx=\"110\" cy=\"155\" r=\"15\" fill=\"#4F4F4F\"></circle>\n    <circle cx=\"110\" cy=\"155\" r=\"8\" fill=\"#696969\"></circle>\n    <circle cx=\"110\" cy=\"155\" r=\"3\" fill=\"#2F2F2F\"></circle>\n    <!-- Animated wheel spokes -->\n    <g>\n      <animateTransform attributeName=\"transform\" type=\"rotate\" values=\"0 110 155;360 110 155\" dur=\"1s\" repeatCount=\"indefinite\"></animateTransform>\n      <line x1=\"110\" y1=\"140\" x2=\"110\" y2=\"170\" stroke=\"#A9A9A9\" stroke-width=\"2\"></line>\n      <line x1=\"125\" y1=\"155\" x2=\"95\" y2=\"155\" stroke=\"#A9A9A9\" stroke-width=\"2\"></line>\n      <line x1=\"121\" y1=\"144\" x2=\"99\" y2=\"166\" stroke=\"#A9A9A9\" stroke-width=\"2\"></line>\n      <line x1=\"121\" y1=\"166\" x2=\"99\" y2=\"144\" stroke=\"#A9A9A9\" stroke-width=\"2\"></line>\n    </g>\n  </g>\n  \n  <!-- Stop sign arm (folded) -->\n  <rect x=\"67\" y=\"90\" width=\"3\" height=\"15\" fill=\"#000\"></rect>\n  <polygon points=\"55,95 67,90 67,100\" fill=\"#FF0000\" stroke=\"#000\" stroke-width=\"1\"></polygon>\n  <text x=\"61\" y=\"98\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"6\" font-weight=\"bold\" fill=\"#FFF\">STOP</text>\n  \n  <!-- License plate -->\n  <rect x=\"62\" y=\"125\" width=\"18\" height=\"8\" rx=\"1\" ry=\"1\" fill=\"#FFF\" stroke=\"#000\" stroke-width=\"1\"></rect>\n  <text x=\"71\" y=\"131\" text-anchor=\"middle\" font-family=\"Arial, sans-serif\" font-size=\"4\" fill=\"#000\">BUS 123</text>\n  \n  <!-- Side mirrors -->\n  <ellipse cx=\"362\" cy=\"82\" rx=\"3\" ry=\"2\" fill=\"#C0C0C0\" stroke=\"#000\" stroke-width=\"1\"></ellipse>\n  <ellipse cx=\"362\" cy=\"98\" rx=\"3\" ry=\"2\" fill=\"#C0C0C0\" stroke=\"#000\" stroke-width=\"1\"></ellipse>\n</svg>"
